{
  "cli": {
    "version": ">= 3.14.0"
  },

  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    },

    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      }
    },

    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "APP_VARIANT": "production"
      }
    }
  }
}
