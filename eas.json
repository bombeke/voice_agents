{
  "cli": {
    "version": ">= 3.14.0",
    "appVersionSource": "remote"
  },

  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "pnpm": "9.15.4",
      "env": {
        "APP_VARIANT": "development"
      }
    },

    "preview": {
      "distribution": "internal",
      "pnpm": "9.15.4",
      "env": {
        "APP_VARIANT": "preview"
      }
    },

    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "pnpm": "9.15.4",
      "env": {
        "APP_VARIANT": "production"
      }
    }
  }
}
